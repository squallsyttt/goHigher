package main

import "fmt"

type CalculateType func(int, int)

func (c CalculateType) Serve() {
	fmt.Println("我是一个函数类型")
}

func add(a, b int) {
	fmt.Println(a + b)
}
func mul(a, b int) {
	fmt.Println(a * b)
}

func main() {
	a := CalculateType(add) //将add函数强制转换成CalculateType类型
	b := CalculateType(mul) //将mul函数强制转换成CalculateType类型

	a(2, 3)
	b(2, 3)
	a.Serve()
	b.Serve()
}
